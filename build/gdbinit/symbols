# Load esp32h2 ROM ELF symbols
define target hookpost-remote
set confirm off
  # if $_streq((char *) 0x4001c7dc, "Nov  1 2022")
  if (*(int*) 0x4001c7dc) == 0x20766f4e && (*(int*) 0x4001c7e0) == 0x32203120 && (*(int*) 0x4001c7e4) == 0x323230
    add-symbol-file c:/esp/esp-zigbee-sdk/tools/esp-rom-elfs/20241011/esp32h2_rev0_rom.elf
  else
    echo Warning: Unknown esp32h2 ROM revision.\n
  end
set confirm on
end


# Load bootloader symbols
set confirm off
    add-symbol-file C:/esp/esp-zigbee-sdk/examples/esp_zigbee_HA_sample/NUOS_HA_Zigbee_Controller_Code/build/bootloader/bootloader.elf
set confirm on

# Load application symbols
file C:/esp/esp-zigbee-sdk/examples/esp_zigbee_HA_sample/NUOS_HA_Zigbee_Controller_Code/build/on_off_light_bulb.elf
